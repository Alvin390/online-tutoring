{
  "name": "online-tutoring",
  "version": "1.0.0",
  "description": "A beautiful, modern web application for managing student registrations and Zoom class access with international phone number support.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{js,jsx,css,scss}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:auth": "vitest run tests/unit/auth.test.js",
    "test:registration": "vitest run tests/unit/registration.test.js",
    "test:dashboard": "vitest run tests/unit/dashboard.test.js",
    "test:all": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alvin390/online-tutoring.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Alvin390/online-tutoring/issues"
  },
  "homepage": "https://github.com/Alvin390/online-tutoring#readme",
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@sentry/react": "^10.26.0",
    "@vercel/analytics": "^1.5.0",
    "@vitejs/plugin-react": "^5.1.1",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "firebase": "^12.6.0",
    "framer-motion": "^12.23.24",
    "jspdf": "^3.0.4",
    "jspdf-autotable": "^5.0.2",
    "react": "^19.2.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.1",
    "react-router-dom": "^7.9.6",
    "vite": "^7.2.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "jsdom": "^27.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.17",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^4.0.10"
  }
